column = 0
while column < 4:
    column += 1
    if column ==1:
        print("*")
    if column == 2:
        print("**")
    if column == 3:
        print("***")
    if column == 4:
        print("****")
while column <8:
    column+=1
    if column == 5:
        print("***")
    if column == 6:
        print("**")
    if column == 7:
        print("*")